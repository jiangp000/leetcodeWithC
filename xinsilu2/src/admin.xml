        <action name="outputDataScoreJSP" class="action.admin.DownloadAllScoreAction" method="downloadAllScoreJSP">
            <result name="success">/WEB-INF/jsp/admin/outputDataScore.jsp</result>
        </action>
        <action name="dataDownloadScore" class="action.admin.DownloadAllScoreAction" method="getDataScoreFile">
            <result name="success">/WEB-INF/jsp/admin/outputDataScoreList.jsp</result>
        </action>

        <action name="outputResultJSP" class="action.admin.ResultGetAction" method="outputResultJSP">
