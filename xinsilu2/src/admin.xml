        </action>

        <action name="addExternelJudge" class="action.admin.JudgerAction" method="addExternelJudge">
            <result name="success">/WEB-INF/jsp/admin/common/success.jsp</result>
        </action>

        <action name="judgeManagerJSP" class="action.admin.JudgerAction" method = "ManageJudgeJSP">
            <result name="success">/WEB-INF/jsp/admin/judgeManager.jsp</result>
        </action>
        <action name="ManageJudge" class="action.admin.JudgerAction" method="ManageJudge">
            <result name="success">/WEB-INF/jsp/admin/common/success.jsp</result>
        </action>
        <action name="removeJudgerJSP" class="action.admin.JudgerAction" method="removeJudgerJSP">
            <result name="success">/WEB-INF/jsp/admin/removeJudger.jsp</result>
        </action>
        <action name="removeJudge" class="action.admin.JudgerAction" method="removeJudge">
