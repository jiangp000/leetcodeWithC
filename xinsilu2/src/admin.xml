            <result name="success">/WEB-INF/jsp/admin/scoreStageControl.jsp</result>
        </action>
        <action name="scoreStageControl" class="action.admin.StageControlAction" method="updateScoreStage">
            <!--<result name="success">/WEB-INF/jsp/admin/outputResultScore.jsp</result>-->
            <result name="noyear" type="redirectAction">　　　　
                <param name="actionName">scoreStageControlJSP</param>
            </result>
        </action>

        <!--撤回评委提交和教师提交-->
        <action name="revokeJudgeSubmitJSP" class="action.admin.StageControlAction" method="revokeJudgeSubmitJSP">
            <result name="success">/WEB-INF/jsp/admin/revokeJudgeSubmitJSP.jsp</result>
        </action>
        <action name="revokeJudgeSubmit" class="action.admin.StageControlAction" method="revokeJudgeSubmit">
            <result name="noyear" type="redirectAction">
                <param name="actionName">revokeJudgeSubmitJSP</param>
            </result>
        </action>
        <action name="revokeTeacherSubmitJSP" class="action.admin.StageControlAction" method="revokeTeacherSubmitJSP">
