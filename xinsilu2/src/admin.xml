        <action name="outputResultScoreJSP" class="action.admin.DownloadAllScoreAction" method="downloadAllScoreJSP">
            <result name="success">/WEB-INF/jsp/admin/outputResultScore.jsp</result>
        </action>
        <action name="outputResultScore" class="action.admin.DownloadAllScoreAction" method="downloadScore">
            <!--<result name="success">/WEB-INF/jsp/admin/outputResultScore.jsp</result>-->
            <result name="noyear" type="redirectAction">　　　　
                <param name="actionName">outputResultScoreJSP</param>
            </result>
        </action>

        <action name="outputPaperScoreJSP" class="action.admin.DownloadAllScoreAction" method="downloadAllScoreJSP">
            <result name="success">/WEB-INF/jsp/admin/outputPaperScore.jsp</result>
        </action>
        <action name="outputPaperScore" class="action.admin.DownloadAllScoreAction" method="downloadPaperScore">
            <result name="success">/WEB-INF/jsp/admin/outputPaperScore.jsp</result>
            <result name="noyear" type="redirectAction">　　　　
