                <param name="actionName">outputResultJSP</param>
            </result>
        </action>

        <action name="outputScoreJSP" class="action.admin.DownloadAllScoreAction" method="downloadAllScoreJSP">
            <result name="success">/WEB-INF/jsp/admin/outputScore.jsp</result>
        </action>
        <action name="outputScore" class="action.admin.DownloadAllScoreAction" method="downloadFinPreScore">
