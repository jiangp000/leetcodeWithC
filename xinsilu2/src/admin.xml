<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
    <constant name="struts.ui.theme" value="simple"/>

    <package name="admin" namespace="/admin" extends="xinsilu">
        <!--设置拦截器-->
        <interceptors>
            <interceptor name="AdminInterceptor" class="action.interceptors.AdminInterceptor"></interceptor>

            <interceptor-stack name="adminStack">
                <interceptor-ref name="defaultStack"></interceptor-ref>
                <interceptor-ref name="AdminInterceptor"></interceptor-ref>
            </interceptor-stack>

        </interceptors>

        <default-interceptor-ref name="adminStack"/>

        <!--设置默认首页-->
        <default-action-ref name="loginJSP"/>

        <global-results>
            <result name="login">/WEB-INF/jsp/admin/login.jsp</result>
            <result name="error">/WEB-INF/jsp/admin/common/error.jsp</result>
            <result name="input">/WEB-INF/jsp/admin/common/input.jsp</result>
            <result name="success">/WEB-INF/jsp/admin/common/success.jsp</result>
        </global-results>

        <!--首页-->

        <action name="loginJSP" class="action.admin.AdminAction">
            <result name="success">/WEB-INF/jsp/admin/login.jsp</result>
        </action>

        <action name="login" class="action.admin.AdminAction" method="login">
            <result name="success">/WEB-INF/jsp/admin/index.jsp</result>
        </action>

        <action name="logout" class="action.admin.AdminAction" method="logout">
            <result name="success">/WEB-INF/jsp/admin/login.jsp</result>
        </action>

        <action name="index">
            <result name="success">/WEB-INF/jsp/admin/index.jsp</result>
        </action>


        <!--新闻管理-->
        <action name="news" class="action.admin.NewsAction" method="news">
            <result name="success">/WEB-INF/jsp/admin/news/news.jsp</result>
        </action>
        <action name="addNewsJSP" class="action.admin.NewsAction">
            <result name="success">/WEB-INF/jsp/admin/news/addNews.jsp</result>
        </action>
        <action name="addNews" class="action.admin.NewsAction" method="addNews">
            <result name="input">/WEB-INF/jsp/admin/news/addNews.jsp</result>
        </action>
        <action name="editNewsJSP" class="action.admin.NewsAction" method="editNewsJSP">
            <result name="success">/WEB-INF/jsp/admin/news/editNews.jsp</result>
        </action>
        <action name="editNews" class="action.admin.NewsAction" method="editNews">
        </action>
        <action name="deleteNews" class="action.admin.NewsAction" method="deleteNews">
        </action>

        <!--附件管理-->
        <action name="files" class="action.admin.FilesAction" method="files">
            <result name="success">/WEB-INF/jsp/admin/files/files.jsp</result>
        </action>
        <action name="addFileJSP" class="action.admin.FilesAction">
            <result name="success">/WEB-INF/jsp/admin/files/addFile.jsp</result>
        </action>
        <action name="addFile" class="action.admin.FilesAction" method="addFile">
            <!--<result name="input" type="redirectAction">　　　　-->
            <!--<param name="actionName">addFileJSP</param>-->
            <!--</result>-->
        </action>
        <action name="deleteFile" class="action.admin.FilesAction" method="deleteFile">
            <result name="input" type="redirectAction">　　　　
                <param name="actionName">files</param>
            </result>
        </action>

        <!--培训管理-->

        <action name="train" class="action.admin.TrainAction" method="train">
            <result name="success">/WEB-INF/jsp/admin/train/train.jsp</result>
        </action>
        <action name="addTrainJSP" class="action.admin.TrainAction">
            <result name="success">/WEB-INF/jsp/admin/train/addTrain.jsp</result>
        </action>
        <action name="addTrain" class="action.admin.TrainAction" method="addTrain">
        </action>
        <action name="editTrainJSP" class="action.admin.TrainAction" method="editTrainJSP">
            <result name="success">/WEB-INF/jsp/admin/train/editTrain.jsp</result>
        </action>
        <action name="editTrain" class="action.admin.TrainAction" method="editTrain">
        </action>
        <action name="deleteTrain" class="action.admin.TrainAction" method="deleteTrain">
        </action>
        <action name="outputTrainJSP" class="action.admin.TrainEnrollAction" method="outputTrainJSP">
            <result name="success">/WEB-INF/jsp/admin/train/outputTrain.jsp</result>
        </action>
        <action name="outputTrain" class="action.admin.TrainEnrollAction" method="outputTrain">
            <result name="success">/WEB-INF/jsp/admin/enrollCSV.jsp</result>
        </action>


        <!--项目管理-->

        <action name="outputProjectJSP" class="action.admin.ProjectEnrollAction" method="outputProjectJSP">
            <result name="success">/WEB-INF/jsp/admin/outputProject.jsp</result>
        </action>
        <action name="outputProject" class="action.admin.ProjectEnrollAction" method="outputProject">
            <result name="success">/WEB-INF/jsp/admin/enrollCSV.jsp</result>
            <result name="noyear" type="redirectAction">　　　　
                <param name="actionName">outputProjectJSP</param>
            </result>
        </action>


        <!--课题研究-->

        <action name="outputResearchJSP" class="action.admin.ResearchEnrollAction" method="outputResearchJSP">
